{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16", // Change this to Node16
    "moduleResolution": "Node16", // Add this line    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
